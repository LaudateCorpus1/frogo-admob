<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frogobox.appadmob">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KickStart">

        <activity
            android:name=".ComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_compose"
            android:theme="@style/Theme.KickStart.NoActionBar" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".mvvm.movie.MovieActivity" />
        <activity android:name=".mvvm.news.NewsActivity" />
        <activity android:name=".mvvm.main.AboutUsActivity" />

        <activity
            android:name=".mvvm.main.MainActivity"
            android:exported="true">

        </activity>

        <!-- Sample AdMob App ID: ca-app-pub-3940256099942544~3347511713 -->
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_publisher_id" />

    </application>

</manifest>