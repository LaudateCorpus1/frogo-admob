<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:padding="@dimen/frogo_dimen_16dp"
    tools:context=".mvvm.main.InterstitialActivity">

    <Button
        android:id="@+id/btn_interstitial"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Easy Interstitial"/>

    <Button
        android:id="@+id/btn_interstitial_callback"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Interstital Callback"/>

</LinearLayout>